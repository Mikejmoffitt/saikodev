	.section	.text

#include "sai/memmap.h"

; a6.l = return
	.global	sai_min_psg_init
sai_min_psg_init:
	; Shut up PSG - On my VA0 units it defaults to a loud noise.
	lea	PSG_BASE, a0
	move.b	#$9F, (a0)  ; Channel 0 attenuation $F (quiet)
	move.b	#$BF, (a0)  ; Channel 1 attenuation $F (quiet)
	move.b	#$DF, (a0)  ; Channel 2 attenuation $F (quiet)
	move.b	#$FF, (a0)  ; Channel 3 attenuation $F (quiet)
	jmp	(a6)
