	.section	.text

#include "sai/memmap.h"

; a6.l = return
	.global	sai_min_psg_init
sai_min_psg_init:
	; Shut up PSG - On my VA0 units it defaults to a loud noise.
	lea	PSG_BASE, a0
	move.b	#$9F, d0  ; Channel 0 att 15
	move.b	#$20, d1  ; Channel offset
	move.b	d0, (a0)
	add.b	d1, d0
	move.b	d0, (a0)
	add.b	d1, d0
	move.b	d0, (a0)
	add.b	d1, d0
	move.b	d0, (a0)
	jmp	(a6)
